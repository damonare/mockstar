{
  "name": "mockstar-cli",
  "version": "0.1.3",
  "description": "CLI tool for MockStar",
  "bin": {
    "mockstar": "./bin/mockstar"
  },
  "scripts": {
    "prepublishOnly": "npm run build-ui",
    "dev-ui": "cd ./biz/webui && npm run start",
    "build-ui": "cd ./biz/webui && npm run build",
    "test-mocha": "node ./node_modules/mocha/bin/_mocha",
    "test": "npm run eslint && npm run test-cov",
    "eslint": "eslint src/",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha"
  },
  "keywords": [
    "mock",
    "server"
  ],
  "author": "helinjiang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockstarjs/mockstar.git"
  },
  "bugs": {
    "url": "https://github.com/mockstarjs/mockstar/issues"
  },
  "homepage": "https://github.com/mockstarjs/mockstar#readme",
  "configs": {
    "compatibleVersion": ">=3.0.10"
  },
  "engines": {
    "node": ">=5.6.0"
  },
  "dependencies": {
    "abbrev": "^1.1.1",
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.12",
    "chalk": "^2.3.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "cross-spawn": "^5.1.0",
    "easy-table": "^1.1.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "figlet": "^1.2.0",
    "fs-extra": "^5.0.0",
    "fs-handler": "^0.1.2",
    "hexo-fs": "^0.2.3",
    "inquirer": "^3.0.6",
    "js-yaml": "^3.11.0",
    "log4js": "^2.8.0",
    "meow": "^3.7.0",
    "method-override": "^2.3.10",
    "minimist": "^1.2.0",
    "mockstar": "^0.1.3",
    "morgan": "^1.9.0",
    "ora": "^1.4.0",
    "os": "^0.1.1",
    "osenv": "^0.1.4",
    "pm2": "^2.10.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "semver": "^5.4.1",
    "socket.io": "^2.1.1",
